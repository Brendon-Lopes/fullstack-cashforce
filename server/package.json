    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
